<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="150.0" prefWidth="300.0" stylesheets="@../../css/export-success-alert.css"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.alan344.controller.dialog.ExportSuccessAlertController">
    <children>
        <Button layoutX="245.0" layoutY="120.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0"
                prefWidth="50.0" text="关闭"/>
        <Text id="text" fx:id="text" layoutX="120.0" layoutY="80.0"/>
        <ImageView fx:id="image" fitHeight="36.0" fitWidth="36.0" layoutX="65.0" layoutY="55.0"/>
        <Button fx:id="openFileButton" layoutX="159.0" layoutY="120.0" mnemonicParsing="false"
                onAction="#openFileAfterExport" prefHeight="25.0" prefWidth="79.0" text="打开文件夹"/>
    </children>
</AnchorPane>
